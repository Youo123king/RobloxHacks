-- تحميل مكتبة Kova
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Youo123king/RobloxHacks/main/Kova.lua"))()

-- إنشاء واجهة المستخدم (GUI)
local window = library.CreateWindow("سكربتات يوسف🌟💥")

-- القسم الأول: Home
local homeTab = window:CreateTab("Home")
homeTab:CreateLabel("اسم اللاعب: " .. game.Players.LocalPlayer.Name)
homeTab:CreateLabel("عمره: 15")
homeTab:CreateLabel("البلد: مصر")
homeTab:CreateLabel("الهاك المستخدم: يمكنك الاختيار من الأدوات المختلفة")
homeTab:CreateLabel("صانع السكربت: owner youssefgamingor")

-- القسم الثاني: Player
local playerTab = window:CreateTab("Player")
playerTab:CreateSlider("Speed", 16, 500, 50, function(val)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = val
end)

playerTab:CreateSlider("Jump", 50, 300, 50, function(val)
    game.Players.LocalPlayer.Character.Humanoid.JumpHeight = val
end)

playerTab:CreateSlider("Fly Speed", 50, 300, 50, function(val)
    -- تعديل سرعة الطيران
end)

playerTab:CreateToggle("Infinity Jump", function(state)
    if state then
        -- تفعيل Infinite Jump
    else
        -- إيقاف Infinite Jump
    end
end)

playerTab:CreateToggle("ESP", function(state)
    if state then
        -- تفعيل ESP
    else
        -- إيقاف ESP
    end
end)

playerTab:CreateToggle("X-ray", function(state)
    if state then
        -- تفعيل X-ray
    else
        -- إيقاف X-ray
    end
end)

playerTab:CreateButton("Rejoin", function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
end)

